.model small
.stack 100h

.data
num db '413$', 0
newNum db 10 dup(0)

.code
main:
    mov ax, @data
    mov ds, ax            

    lea si, num
    lea di, newNum

    shift_right:
    mov al, [si]
    cmp al, '$'           
    je done
    mov dl, al
    mov ah, 02h
    int 21h

    inc si
    jmp shift_right

done:
    mov ah, 4Ch
    int 21h

end main
